{
  "name": "image-processor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && npm run postbuild",
    "postbuild": "copyfiles -u 1 public/**/* dist/",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run build && jasmine --config=spec/support/jasmine.json",
    "prettier": "prettier --check \"src/**\"",
    "eslint": "eslint \"src/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codespaces-org/image-processor.git"
  },
  "author": "Youssef Sobhy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codespaces-org/image-processor/issues"
  },
  "homepage": "https://github.com/codespaces-org/image-processor#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.4",
    "@types/sharp": "^0.29.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^4.6.0",
    "sharp": "^0.29.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.10.3",
    "@types/supertest": "^2.0.11",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "concurrently": "^6.5.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.0",
    "typescript": "^4.5.4"
  }
}
